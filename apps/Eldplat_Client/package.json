{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:compile-lang": "npm-run-all --parallel dev lang:compile",
    "build": "next build",
    "build:compile-lang": "npm-run-all --parallel lang:compile build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "lint:error": "next lint --quiet",
    "lint:fix": "next lint --fix",
    "codegen": "graphql-codegen --config codegen.ts",
    "lang:extract": "formatjs extract \"src/**/*.{js,jsx,ts,tsx}\" --ignore \"src/**/*.d.{ts}\" --out-file lang/en.json --id-interpolation-pattern [contenthash:5]",
    "lang:compile": "formatjs compile-folder --ast lang compiled-lang"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/error-message": "^2.0.1",
    "@types/node": "18.11.17",
    "classnames": "^2.3.2",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.8",
    "deepmerge": "^4.2.2",
    "evergreen-ui": "^7.1.8",
    "lodash": "^4.17.21",
    "next": "^13.1.2",
    "next-auth": "^4.24.3",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^4.11.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-intl": "^6.4.1",
    "react-select": "^5.7.3",
    "sass": "^1.58.3",
    "sharp": "^0.32.1",
    "styled-components": "^5.3.6",
    "swr": "^2.2.4",
    "typescript": "4.9.4",
    "uuid": "^9.0.0",
    "zod": "^3.21.4",
    "zustand": "^4.3.6",
    "baseui": "*"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.12",
    "@graphql-codegen/cli": "2.16.1",
    "@graphql-codegen/client-preset": "^1.2.3",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.2.42",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-datepicker": "^4.8.0",
    "@types/react-dom": "^18.2.17",
    "@types/styled-components": "^5.1.32",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-plugin-formatjs": "^10.3.35",
    "eslint": "^8.38.0",
    "eslint-config-next": "^13.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.7"
  },
  "optionalDependencies": {
    "fsevents": "*"
  }
}
